package enties;

public class conta {
	
	private int conta;
	private String nome;
	private double saldo;
	
	public conta(int conta, String nome, double saldo) {
	
		this.conta = conta;
		this.nome = nome;
		this.saldo = saldo;
	}
	
	public conta(int conta, String nome) {
		super();
		this.conta = conta;
		this.nome = nome;
	}

	public int getConta() {
		return conta;
	}


	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public double getSaldo() {
		return saldo;
	}

	public void deposito(double deposito) {
		
	}
	
	
	

}
