public class Datas {

	public static void main(String[] args) {
		
		SimpleDateFormat sdf = new SimpleDateFormat("dd/mm/aaaa");
		Date d = Date.from(Instant.parse("2019/01/16"));
		
		System.out.println(d);
		

	}

}
