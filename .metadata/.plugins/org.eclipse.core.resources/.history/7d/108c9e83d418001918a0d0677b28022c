package application;

import java.util.ArrayList;
import java.util.List;

public class Listas {

	public static void main(String[] args) {
	
		List<String> list = new ArrayList<>();
		
		list.add("Alessandro");
		list.add("Vanessa");
		list.add("Gabriel");
		list.add("Gabriella");
		
		list.add(2, "pimpim");
		
		System.out.println(list.size());
		
		list.remove("Vanessa");
		
		for (String x : list) {
			
			System.out.println(x);
		}
	}

}
